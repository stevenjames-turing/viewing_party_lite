<div class="row" align="center" id="discover_movies">
      <%= button_to("Discover Movies", discover_index_path, method: :get)%>
</div>
<br>
<br>
<ol>
  <% if @data != nil %>
    <% @data.each do |movie| %>
      <li>
       <%= link_to movie.title, movie_path(movie.id) %>
        - (Vote Average: <%= movie.vote_average %>)
      </li>
    <% end  %>
<% end  %>
</ol>
